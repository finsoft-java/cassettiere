{
  "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true
  },
  "extends": [
      "airbnb-base"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": 12
  },
  "plugins": [
      "@typescript-eslint"
  ],
  "rules": {
      "arrow-parens": "off",
      "comma-dangle": "off",
      "import/extensions": "off",
      "import/prefer-default-export": "off",
      "import/no-unresolved": "off",
      "linebreak-style": "off",
      "lines-between-class-members": "off",
      "max-len": ["error", { "code": 140} ],
      "no-plusplus": "off",
      "no-unused-vars": "warn",
      "prefer-template": "off"
  }
}
